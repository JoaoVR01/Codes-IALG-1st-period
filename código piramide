#include <iostream>
using namespace std;
int main(){
/*
 para x = 5
 linha1 = i=2
 linha2 = de i=1 a i=3 
  
 */
	int x,y,i,j;
	cout<<"insira o tamanho do quadrado: ";
	cin>>x;
	y = 2*x;
	char matrix[x][y];
	
	for(i = 0; i<2*x;i++){
		for(j = 0;j<y;j++){
			matrix[i][j] = '*';
		}
	}
	
	for(i = 0; i<x;i++){
		for(j = 0;j<y;j++){
			cout<<matrix[i][j];
		}
		cout<<endl;
	}
	
	return 0;
}
