#include <iostream>
#include <cmath>

using namespace std;

int main(){
	float valor1 = 3.14, valor2 = 3.5;
	cout << round(valor1) << endl; //função de arredondamento
	cout << round(valor2) << endl;
	cout << ceil(valor1) << endl; //arredonda para o proximo inteiro 
	cout << floor(valor1) << endl; //arredonda para o inteiro anterior
	
	return 0;
}
