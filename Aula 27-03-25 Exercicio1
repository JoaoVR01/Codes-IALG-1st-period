//Cálculo de distância entre dois pontos no plano

#include <iostream>
#include <cmath>

using namespace std;


int main(){
	//Declaração das variaveis dos pontos
	float x1, y1, x2, y2;
	cin >> x1 >> y1 >> x2 >> y2;
	
	//Declaração da variável distância
	float d;
	
	//Cálculo da distância
	d = sqrt(pow(x2-x1,2) + pow(y2-y1,2));
	cout << d;
	
	return 0;
}
